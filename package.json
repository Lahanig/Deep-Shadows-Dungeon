{
  "name": "deep-shadows-dungeon",
  "version": "0.0.31",
  "description": "CLA RogueLike on typescript",
  "main": "./build/index.js",
  "bin": "./build/index.js",
  "pkg": {
    "assets": [
      "content/**/*"
    ],
    "scripts": [
      "build/**/*.js"
    ],
    "targets": [
      "node18-linux",
      "node18-win"
    ],
    "outputPath": "./dist"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./build/index.js",
    "dev": "tsc -w",
    "compile": "tsc",
    "build": "pkg --config ./package.json ./build/index.js"
  },
  "keywords": [
    "Game",
    "CLA",
    "RogueLike"
  ],
  "author": "Kaibi",
  "license": "ISC",
  "devDependencies": {
    "node-key-sender": "^1.0.11",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "boxednode": "^2.4.3",
    "keypress": "^0.2.1",
    "nodemon": "^3.1.3",
    "pkg": "^5.8.1"
  }
}
